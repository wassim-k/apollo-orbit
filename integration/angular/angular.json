{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "core": {
      "projectType": "application",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "preserveSymlinks": true,
            "outputPath": "dist/core",
            "index": "projects/core/src/index.html",
            "browser": "projects/core/src/main.ts",
            "tsConfig": "projects/core/tsconfig.app.json",
            "assets": [
              "projects/core/src/favicon.ico",
              "projects/core/src/assets"
            ],
            "styles": [
              "projects/core/src/styles.scss"
            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "decorator-cache-getter",
              "graphql-tag"
            ],
            "server": "projects/core/src/main.server.ts",
            "ssr": {
              "entry": "projects/core/src/server.ts"
            },
            "outputMode": "server"
          },
          "configurations": {
            "development": {},
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/core/src/environments/environment.ts",
                  "with": "projects/core/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "core:build:production"
            },
            "development": {
              "buildTarget": "core:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "core:build"
          }
        }
      }
    },
    "state": {
      "projectType": "application",
      "root": "projects/state",
      "sourceRoot": "projects/state/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "preserveSymlinks": true,
            "outputPath": "dist/state",
            "index": "projects/state/src/index.html",
            "browser": "projects/state/src/main.ts",
            "tsConfig": "projects/state/tsconfig.app.json",
            "assets": [
              "projects/state/src/favicon.ico",
              "projects/state/src/assets",
              {
                "glob": "app-config.json",
                "input": "projects/state/src/app/config",
                "output": "/"
              }
            ],
            "styles": [
              "projects/state/src/styles.scss"
            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "decorator-cache-getter",
              "graphql-tag"
            ]
          },
          "configurations": {
            "development": {},
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/state/src/environments/environment.ts",
                  "with": "projects/state/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "state:build:production"
            },
            "development": {
              "buildTarget": "state:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "state:build"
          }
        }
      }
    }
  },
  "cli": {
    "cache": {
      "enabled": false
    },
    "packageManager": "yarn",
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true,
      "type": "component"
    },
    "@schematics/angular:class": {
      "skipTests": true
    },
    "@schematics/angular:directive": {
      "skipTests": true,
      "type": "directive"
    },
    "@schematics/angular:guard": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "skipTests": true,
      "typeSeparator": "."
    },
    "@schematics/angular:service": {
      "skipTests": true,
      "type": "service"
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
