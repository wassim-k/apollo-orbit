{
  "name": "@integration/angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:core": "concurrently --raw \"yarn codegen:core --watch\" \"ng serve core\"",
    "start:effects": "concurrently --raw \"yarn codegen:effects --watch\" \"ng serve effects\"",
    "build": "ng build",
    "lint": "ng lint",
    "codegen": "yarn codegen:core && yarn codegen:effects",
    "codegen:core": "graphql-codegen --config ./projects/core/codegen.ts",
    "codegen:effects": "graphql-codegen --config ./projects/effects/codegen.ts",
    "codegen:core:inspect": "node --nolazy --inspect-brk ../../node_modules/@graphql-codegen/cli/bin.js --config ./projects/core/codegen.ts",
    "ssr:dev": "ng run core:serve-ssr",
    "ssr:serve": "node dist/core/server/main.js",
    "ssr:build": "ng build && ng run core:server:production",
    "prerender": "ng run core:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-server": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@apollo-orbit/angular": "*",
    "@apollo/client": "^3.7.11",
    "@ngneat/until-destroy": "^9.2.3",
    "@nguniversal/express-engine": "^15.2.0",
    "bufferutil": "^4.0.7",
    "decorator-cache-getter": "^1.0.0",
    "express": "^4.18.2",
    "rxjs": "^7.8.0",
    "toastify-js": "1.12.0",
    "tslib": "^2.5.0",
    "utf-8-validate": "^6.0.3",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@angular/language-service": "^15.2.5",
    "@apollo-orbit/codegen": "*",
    "@nguniversal/builders": "^15.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11"
  }
}
