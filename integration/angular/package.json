{
  "name": "@integration/angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:core": "concurrently --raw \"yarn codegen:core --watch\" \"ng serve core\"",
    "start:effects": "concurrently --raw \"yarn codegen:effects --watch\" \"ng serve effects\"",
    "build": "ng build",
    "lint": "ng lint",
    "codegen": "yarn codegen:core && yarn codegen:effects",
    "codegen:core": "graphql-codegen --config ./projects/core/codegen.yml",
    "codegen:effects": "graphql-codegen --config ./projects/effects/codegen.yml",
    "codegen:core:inspect": "node --nolazy --inspect-brk ../../node_modules/@graphql-codegen/cli/bin.js --config ./projects/core/codegen.yml",
    "ssr:dev": "ng run core:serve-ssr",
    "ssr:serve": "node dist/core/server/main.js",
    "ssr:build": "ng build && ng run core:server:production",
    "prerender": "ng run core:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/forms": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-server": "^13.3.6",
    "@angular/router": "^13.3.6",
    "@apollo-orbit/angular": "*",
    "@apollo/client": "^3.6.2",
    "@ngneat/until-destroy": "^9.1.2",
    "@nguniversal/express-engine": "^13.1.1",
    "bufferutil": "^4.0.6",
    "decorator-cache-getter": "^1.0.0",
    "express": "^4.18.1",
    "rxjs": "^7.5.5",
    "toastify-js": "1.11.2",
    "tslib": "^2.4.0",
    "utf-8-validate": "^5.0.9",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.6",
    "@angular/language-service": "^13.3.6",
    "@apollo-orbit/codegen": "*",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0"
  }
}
