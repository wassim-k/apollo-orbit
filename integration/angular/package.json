{
  "name": "@integration/angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:core": "concurrently --raw \"yarn codegen:core --watch\" \"ng serve core\"",
    "start:effects": "concurrently --raw \"yarn codegen:effects --watch\" \"ng serve effects\"",
    "build": "ng build",
    "lint": "ng lint",
    "codegen": "yarn codegen:core && yarn codegen:effects",
    "codegen:core": "graphql-codegen --config ./projects/core/codegen.yml",
    "codegen:effects": "graphql-codegen --config ./projects/effects/codegen.yml",
    "codegen:core:inspect": "node --nolazy --inspect-brk ../../node_modules/@graphql-codegen/cli/bin.js --config ./projects/core/codegen.yml",
    "ssr:dev": "ng run core:serve-ssr",
    "ssr:serve": "node dist/core/server/main.js",
    "ssr:build": "ng build && ng run core:server:production",
    "prerender": "ng run core:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.2",
    "@angular/common": "^12.2.2",
    "@angular/compiler": "^12.2.2",
    "@angular/core": "^12.2.2",
    "@angular/forms": "^12.2.2",
    "@angular/platform-browser-dynamic": "^12.2.2",
    "@angular/platform-browser": "^12.2.2",
    "@angular/platform-server": "^12.2.2",
    "@angular/router": "^12.2.2",
    "@apollo-orbit/angular": "*",
    "@apollo/client": "^3.4.8",
    "@ngneat/until-destroy": "^8.1.1",
    "@nguniversal/express-engine": "^12.1.0",
    "bufferutil": "^4.0.3",
    "decorator-cache-getter": "^1.0.0",
    "express": "^4.17.1",
    "rxjs": "^6.6.5",
    "toastify-js": "1.11.1",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "^12.2.2",
    "@angular/compiler-cli": "^12.2.2",
    "@angular/language-service": "^12.2.2",
    "@apollo-orbit/codegen": "*",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "~12.12.50"
  }
}
