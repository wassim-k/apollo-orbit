{
  "name": "@integration/angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:core": "concurrently --raw \"yarn codegen:core --watch\" \"ng serve core\"",
    "start:effects": "concurrently --raw \"yarn codegen:effects --watch\" \"ng serve effects\"",
    "build": "ng build",
    "lint": "ng lint",
    "codegen": "yarn codegen:core && yarn codegen:effects",
    "codegen:core": "graphql-codegen --config ./projects/core/codegen.yml",
    "codegen:effects": "graphql-codegen --config ./projects/effects/codegen.yml",
    "codegen:core:inspect": "node --nolazy --inspect-brk ../../node_modules/@graphql-codegen/cli/bin.js --config ./projects/core/codegen.yml",
    "ssr:dev": "ng run core:serve-ssr",
    "ssr:serve": "node dist/core/server/main.js",
    "ssr:build": "ng build && ng run core:server:production",
    "prerender": "ng run core:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/forms": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-server": "^15.0.3",
    "@angular/router": "^15.0.3",
    "@apollo-orbit/angular": "*",
    "@apollo/client": "^3.7.2",
    "@ngneat/until-destroy": "^9.2.2",
    "@nguniversal/express-engine": "^15.0.0",
    "bufferutil": "^4.0.7",
    "decorator-cache-getter": "^1.0.0",
    "express": "^4.18.2",
    "rxjs": "^7.6.0",
    "toastify-js": "1.12.0",
    "tslib": "^2.4.1",
    "utf-8-validate": "^5.0.10",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular/cli": "^15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@angular/language-service": "^15.0.3",
    "@apollo-orbit/codegen": "*",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.13"
  }
}