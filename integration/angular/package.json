{
  "name": "@integration/angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:core": "concurrently --raw \"yarn codegen:core --watch\" \"ng serve core\"",
    "start:state": "concurrently --raw \"yarn codegen:state --watch\" \"ng serve state\"",
    "build": "ng build",
    "lint": "ng lint",
    "codegen": "yarn codegen:core && yarn codegen:state",
    "codegen:core": "graphql-codegen --config ./projects/core/codegen.ts",
    "codegen:state": "graphql-codegen --config ./projects/state/codegen.ts",
    "codegen:core:inspect": "node --nolazy --inspect-brk ../../node_modules/@graphql-codegen/cli/bin.js --config ./projects/core/codegen.ts",
    "serve:ssr:core": "node dist/core/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.2.3",
    "@angular/platform-server": "^20.2.3",
    "@angular/router": "^20.2.3",
    "@angular/ssr": "^20.2.1",
    "@apollo-orbit/angular": "*",
    "bufferutil": "^4.0.9",
    "crypto-hash": "^3.1.0",
    "decorator-cache-getter": "^1.0.0",
    "express": "^5.1.0",
    "rxjs": "^7.8.2",
    "toastify-js": "1.12.0",
    "tslib": "^2.8.1",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@angular/language-service": "^20.2.3",
    "@apollo-orbit/codegen": "*",
    "@parcel/watcher": "^2.5.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/toastify-js": "^1.12.4"
  }
}