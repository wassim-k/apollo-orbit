{
  "name": "@apollo-orbit/react",
  "version": "2.0.0",
  "description": "Apollo Client (React) modular state management abstraction / flux architecture",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wassim-k/apollo-orbit.git"
  },
  "keywords": [
    "apollo",
    "@apollo/client",
    "orbit",
    "react",
    "graphql",
    "redux",
    "state",
    "state-management"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "build": "ng-packagr --config ./tsconfig.lib.json -p ng-package.json",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "codegen:test": "graphql-codegen --config ./tests/graphql/codegen.ts"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.0",
    "graphql": ">=0.11",
    "react": ">=16.0.0"
  },
  "dependencies": {
    "@apollo-orbit/core": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "~10.4.1",
    "@testing-library/react": "^16.3.0"
  }
}
